<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/travel_ontology.owl"
         xmlns="http://example.org/travel_ontology.owl#">

<owl:Ontology rdf:about="http://example.org/travel_ontology.owl"/>

<owl:ObjectProperty rdf:about="#hasPart">
  <rdfs:domain rdf:resource="#TravelService"/>
  <rdfs:range rdf:resource="#TravelService"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#departureCity">
  <rdfs:domain rdf:resource="#FlightService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#arrivalCity">
  <rdfs:domain rdf:resource="#FlightService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#price">
  <rdfs:domain rdf:resource="#TravelService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#airline">
  <rdfs:domain rdf:resource="#FlightService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#location">
  <rdfs:domain rdf:resource="#HotelService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#rating">
  <rdfs:domain rdf:resource="#HotelService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#serviceDate">
  <rdfs:domain rdf:resource="#TravelService"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#TravelService">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#FlightService">
  <rdfs:subClassOf rdf:resource="#TravelService"/>
</owl:Class>

<owl:Class rdf:about="#HotelService">
  <rdfs:subClassOf rdf:resource="#TravelService"/>
</owl:Class>

<owl:Class rdf:about="#TaxiService">
  <rdfs:subClassOf rdf:resource="#TravelService"/>
</owl:Class>


</rdf:RDF>
